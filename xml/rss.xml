---
layout: none
extension: xml
filter: erb
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:trackback="http://madskills.com/public/xml/rss/module/trackback/">
  <channel>
    <title>deferred until inspiration hits</title>
    <link>http://chrisroos.co.uk/</link>
    <language>en-us</language>
    <ttl>40</ttl>

    <description></description>
    {% for post in site.posts %}
      <item>
        <title>{{ post.title }}</title>
        <description>{{ post.content | escape }}</description>

        <pubDate>{{ post.created_at | date_to_rfc822 }}</pubDate>
        <guid isPermaLink="false">urn:uuid:{{ post.guid }}</guid>
        <author>Chris Roos</author>
        <link>http://chrisroos.co.uk{{ post.url }}</link>
      </item>
    {% endfor %}
  </channel>
</rss>
